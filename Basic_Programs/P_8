# include<stdio.h>
void main(){
    int a;
    printf("Enter a Number to be checked: ");
    scanf("%d",&a);
    if(a<10){
        printf("Number is less than 10");
    }else if(a>=10 && a<20){
        printf("Number is greater than or equal to 10 and less than 20");
    }else if(a>=20 && a<30){
        printf("Number is greater than or equal to 20 and less than 30");
    }else if(a>=30 && a<40){
        printf("Number is greater than or equal to 30 and less than 40");
    }
}
