# include<stdio.h>
void main(){
    int a,b,c;
    printf("Enter three numbers: ");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b & a>c){
        if(b>c){
            printf("Second largest number: %d",b);
        }else if(c>b){
            printf("Second largest number: %d",c);
        }
    }else if(b>a && b>c){
        if(a>c){
            printf("Second largest number: %d",a);
        }else if(c>a){
            printf("Second largest number: %d",c);
        }
    }else if(c>a && c>b){
        if(a>b){
            printf("Second largest number: %d",a);
        }else if(b>a){
            printf("Second largest number: %d",b);
        }
    }
}
