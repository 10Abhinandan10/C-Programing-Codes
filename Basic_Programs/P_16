# include<stdio.h>
void main(){
    int a,b,c;
    printf("Enter three sides of a triangle: ");
    scanf("%d %d %d",&a,&b,&c);
    if(a==b && b==c && a==c){
        printf("Triangle is Equilateral");
    }else if(a==b || a==c || b==c){
        printf("Triangle is Isoceles");
    }else if(a!=b && a!=c && b!=c){
        printf("Triangle is Scalene");
    }

}
