# include<stdio.h>
void main(){
    int a,b,c;
    printf("Enter 1st number: ");
    scanf("%d",&a);
    printf("Enter 2nd number: ");
    scanf("%d",&b);
    printf("Enter 3rd number: ");
    scanf("%d",&c);
    if(a==b && a==c && b==c){
        printf("The numbers are equal\n");
    }else if(a!=b && a!=c && b!=c){
        printf("The numbers are not equal\n");
        if(a>b && a>c){
            printf("Largest number among them: %d",a);
        }else if(b>a && b>c){
            printf("Largest number among them: %d",b);
        }else if(c>a &&c>b){
            printf("Largest number among them: %d",c);
        }
    }
}
